			document.addEventListener('DOMContentLoaded', () => {
				const video = document.querySelector('video[data-src]');
				const observer = new IntersectionObserver((entries) => {
					entries.forEach(entry => {
						if (entry.isIntersecting) {
							video.src = video.getAttribute('data-src');
							observer.disconnect();
						}
					});
				});
				observer.observe(video);
			});
		</script><script>
			const marqueeContent = document.querySelector('.marquee-content');
			let clone = marqueeContent.cloneNode(true);
			marqueeContent.parentElement.appendChild(clone);

			let marquee = document.querySelector('.marquee');
			let speed = 0.3;
			let marqueeWidth = marquee.scrollWidth;

			let position = 0;

			function scrollMarquee() {
				position -= speed;
				if (Math.abs(position) >= marqueeWidth / 2) {
				position = 0;
				}
				marqueeContent.style.transform = `translateX(${position}px)`;
				clone.style.transform = `translateX(${position}px)`;
				requestAnimationFrame(scrollMarquee);
			}

			marquee.addEventListener('mouseover', () => {
				speed = 0;
			});

			marquee.addEventListener('mouseout', () => {
				speed = 0.3;
			});

			scrollMarquee();
		</script><script>
			document.addEventListener('DOMContentLoaded', function() {
				const slider = document.getElementById('storageSlider');
				const costDisplay = document.getElementById('costDisplay');
				const progressBar = document.getElementById('progressBar');
				const sliderThumb = document.getElementById('sliderThumb');
				const freeBadge = document.getElementById('freeBadge');
				const graduationsContainer = document.getElementById('graduations');
				const tbValuesContainer = document.getElementById('tbValues');
				const setupPlanButton = document.getElementById('setupPlanButton');
				const getInTouchText = document.getElementById('getInTouchText');

				const graduations = [1, 5, 10, 15, 20, 25, 30, 35, 40, 45, 50];

				graduations.forEach(tb => {
					const graduation = document.createElement('div');
					graduation.className = 'absolute w-px h-5 bg-white/20';
					graduation.style.left = `${(tb/52) * 100}%`;
					graduation.style.top = '52%';
					graduation.style.transform = 'translateY(-52%)';
					graduationsContainer.appendChild(graduation);

					const tbValue = document.createElement('span');
					tbValue.textContent = `${tb}TiB`;
					tbValue.style.position = 'absolute';
					tbValue.style.left = `${(tb/52) * 100}%`;
					tbValue.style.transform = 'translateX(-52%)';
					tbValuesContainer.appendChild(tbValue);
				});

				function calculateCost(tb) {
					if (tb <= 1) {
					return 'FREE';
					}
					const billedTb = tb - 1;
					const billedGiB = billedTb * 1024;
					const cost = billedGiB * 0.025;
					return `$${cost.toFixed(2)}`;
				}

				slider.addEventListener('input', () => {
					const value = parseInt(slider.value);
					const percentage = (value / 50) * 100;

					const cost = calculateCost(value);
					costDisplay.textContent = cost;
					progressBar.style.width = `${percentage}%`;
					sliderThumb.style.left = `${percentage}%`;

					if (freeBadge) {
					freeBadge.style.display = value <= 1 ? 'flex' : 'none';
					}

					if (value > 40) {
						setupPlanButton.innerText = "Get in Touch";
						setupPlanButton.classList.add('get-in-touch-slider');
						setupPlanButton.href = "https://forms.gle/rEJQaaDJAC1UMPhy6";
						setupPlanButton.target = "_blank";
						getInTouchText.classList.remove('hidden');
					} else {
						setupPlanButton.innerText = "Set up plan";
						setupPlanButton.classList.remove('get-in-touch-slider');
						getInTouchText.classList.add('hidden');
						setupPlanButton.href = "https://thegraph.market";
						setupPlanButton.target = "_blank";
					}
				});
			});
		</script></body><script>
			document.addEventListener("DOMContentLoaded", function () {
				const stickyContainer = document.getElementById("stickyContainer");
				const offset = stickyContainer.offsetTop;

				window.addEventListener("scroll", function () {
					if (window.innerWidth >= 768) {
						if (window.scrollY >= offset) {
							stickyContainer.classList.add("sticky-package");
						} else {
							stickyContainer.classList.remove("sticky-package");
						}
					} else {
						stickyContainer.classList.remove("sticky-package");
					}
				});
			});
		</script></html>